{
  "src": "assets/",
  "dest": "build/",
  "ghpages": "build/",
  "images": [
    "build/images/**/*"
  ],
  "svg": [
    "build/svg/**/*"
  ],
  "icons": [
    "build/icons/**/*"
  ],
  "iconsFontName": "icon",
  "fonts": [
    "build/fonts/**/*"
  ],
  "vendors": {
    "css": [
      "libs/flickity/dist/flickity.css"
    ],
    "js": [
      "libs/flickity/dist/flickity.pkgd.js"
    ]
  },
  "bundles": {
    "js": [
      {
        "name": "bisoux",
        "src": "components/base.js"
      }
    ],
    "scss": [
      {
        "name": "choux",
        "src": "components/base.scss"
      },
      {
        "name": "cailloux",
        "src": "components/basis.scss"
      }
    ]
  },
  "theme": {
  },
  "singles": [{
    "src": "./yarn.lock",
    "dest": "./build/css"
  }],
  "reader_path": "http://localhost/toolbox-env/toolbox-reader/build/static"
}
